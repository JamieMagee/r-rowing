{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "msg",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "oarspotter-image",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "blob",
      "type": "blob",
      "path": "flair/{src}",
      "direction": "out",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "$return",
      "queueName": "emoji",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
